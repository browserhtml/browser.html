 export type ID = string
 export type Revision = string
 export type URI = string
 export type Time = number

export type Model =
  { type: "Visit"
  , start: Time
  , end: ?Time
  , id: ID
  , device: string
  }

declare export function begin
  (tabID:ID, time:Time, device:string):
  Model

declare export function end
  (model:Model):
  Model
